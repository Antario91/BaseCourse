<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="customerEndpoint" class="webservice.endpoints.CustomerEndpoint" autowire="constructor"/>

    <bean id="orderEndpoint" class="webservice.endpoints.OrderEndpoint" autowire="constructor"/>

    <bean id="productEndpoint" class="webservice.endpoints.ProductEndpoint" autowire="constructor"/>

</beans>